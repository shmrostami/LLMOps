    "import os, requests\n",
    "u = os.environ.get(\"UPLOAD_URL\")\n",
    "for p in (\"/upload\", \"/download/test\"):\n",
    "    try:\n",
    "        r = requests.get(u+p, timeout=5, verify=True)\n",
    "        print(p, r.status_code, r.headers.get(\"server\"))\n",
    "    except Exception as e:\n",
    "        print(p, \"ERR\", repr(e))"
   ]
